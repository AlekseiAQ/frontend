{
  "APP": {
    "api_prefix": "http://127.0.0.1:5000/api",
    "pageIdMeta": "pageName",
    "pages": {
      "raceList": {
        "path": "/index.html",
        "displayName": "Home",
        "resource": "races",
        "renderMap": [
          {"template": "racesTemplate", "container": "races-container"}
        ]
      },
      "raceDetail": {
        "path": ""
      },
      "candidateDetail": {
        "path": "/candidate-detail.html",
        "displayName": "Candidates",
        "mainContainer": "pageContent",
        "mainTemplateId": "candidateTemplate",
        "notFoundTemplateId": "notFoundTemplate",
        "errorTemplateId": "errorTemplate"
      }
    }
  },
  "development": {
    "raceDetail": {
      "path": "/race-detail.html",
      "renderMap": [
        {"template": "raceDetailTemplate", "container": "detail-container"}
      ]
    }
  }
}
